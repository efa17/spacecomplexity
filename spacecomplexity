#include <stdio.h>

int main() {
    int Square (int n){
        printf("%d Bytes", sizeof(int));
        printf("%d Bytes", sizeof(n));
        return n*n;
    }
    int main(){
        int num;
        printf("enter a num: ");
        scanf("%d",&num);
        int value= Square(num);
        printf("%d",value);
    }
    

    
